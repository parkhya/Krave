<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/left_menu_top_layout_width"
    android:layout_height="match_parent"
    android:background="#414042"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/profile"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/ico_users" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Profile"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/findDudes"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/ico_search" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Find Guys"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/dailyKrave"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/ico_cal" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Daily Krave"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/chatMatches"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left"
        android:visibility="visible" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/ico_chat" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Chat / Matches"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252"
        android:visibility="visible" />

    <LinearLayout
        android:id="@+id/setting"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/gear" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Setting"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/contactUs"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:paddingBottom="3dp"
            android:paddingTop="3dp"
            android:scaleType="fitXY"
            android:src="@drawable/contact_us" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Contact Us"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/logout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/left_menu_icon_width"
            android:layout_height="@dimen/left_menu_icon_height"
            android:layout_marginLeft="@dimen/left_menu_icon_layout_margin_left"
            android:scaleType="fitXY"
            android:src="@drawable/logout" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/left_menu_text_layout_margin_left"
            android:text="Logout"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/left_menu_text_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:orientation="vertical" >

            <!--
                 <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/left_menu_icon_arrow_layout_margin_right"
                android:src="@drawable/arrow" />
            -->
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#525252" />

    <LinearLayout
        android:id="@+id/visits"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center|left"
        android:visibility="invisible" >
    </LinearLayout>

</LinearLayout>